<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context="com.snaptechnology.bgonzalez.bookmyroomandroid.activity.HomeFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center">

        <com.snaptechnology.bgonzalez.bookmyroomandroid.SimpleCircularProgressbar
            xmlns:custom="http://schemas.android.com/apk/res-auto"
            custom:progressbar_thickness="10"
            custom:progress="50"
            custom:rotation="anticlockwise"
            custom:progressColor="#FF00FF"
            android:layout_width="200dp"
            android:layout_height="200dp"/>

        <com.github.lzyzsd.circleprogress.DonutProgress
            android:layout_marginLeft="50dp"
            android:id="@+id/donut_progress"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            custom:donut_progress="30"/>

        <TextView
            android:id="@+id/socket"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textSize="32dp"
            android:text="Socket"
            android:textColor="@color/colorPrimary"
            android:textStyle="bold"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true" />


    </RelativeLayout>




</RelativeLayout>